{
  "name": "davispeixoto/php-batch",
  "description": "A framework for PHP batch processing",
  "keywords": [
    "php",
    "batch processing",
    "offline processing"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Davis Peixoto",
      "email": "davis.peixoto@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "psr/log": "~1.0",
    "symfony/config": "2.8.x-dev",
    "symfony/console": "2.8.x-dev",
    "symfony/validator": "2.8.x-dev",
    "symfony/process": "2.8.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "suggest": {
    "monolog/monolog": "For logging utilities",
    "aws/aws-sdk-php": "Allow sending log messages to AWS services like DynamoDB"
  },
  "autoload": {
    "psr-4": {
      "Davispeixoto\\PhpBatch\\": "src/Davispeixoto/PhpBatch"
    }
  },
  "minimum-stability": "dev"
}
